{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "vite",
  "installCommand": "npm install",
  "buildCommand": "ROLLUP_DISABLE_NATIVE=1 npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/contact.ts": {
      "runtime": "nodejs20.x",
      "regions": ["hnd1"]
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
